v0x55dbfc79a3b0_0 .var "reset", 0 0;
v0x55dbfc79a480_0 .net "value", 7 0, v0x55dbfc79a0b0_0;  1 drivers
S_0x55dbfc74df20 .scope module, "c1" "counter" 2 13, 3 1 0, S_0x55dbfc74dd90;
 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "reset";
P_0x55dbfc74e100 .param/l "WIDTH" 0 3 2, +C4<00000000000000000000000000001000>;
v0x55dbfc788660_0 .net "clk", 0 0, v0x55dbfc79a2e0_0;  1 drivers
v0x55dbfc79a0b0_0 .var "out", 7 0;
v0x55dbfc79a190_0 .net "reset", 0 0, v0x55dbfc79a3b0_0;  1 drivers
E_0x55dbfc789340 .event posedge, v0x55dbfc79a190_0, v0x55dbfc788660_0;
    .scope S_0x55dbfc74df20;
T_0 ;
    %wait E_0x55dbfc789340;
    %load/vec4 v0x55dbfc79a190_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55dbfc79a0b0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55dbfc79a0b0_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x55dbfc79a0b0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55dbfc74dd90;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dbfc79a3b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dbfc79a2e0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55dbfc74dd90;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dbfc79a3b0_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dbfc79a3b0_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dbfc79a3b0_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dbfc79a3b0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 8 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55dbfc74dd90;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x55dbfc79a2e0_0;
    %nor/r;
    %store/vec4 v0x55dbfc79a2e0_0, 0, 1;
    %jmp T_3;
